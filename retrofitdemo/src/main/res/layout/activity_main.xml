<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:tools="http://schemas.android.com/tools"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical"
              android:paddingBottom="@dimen/activity_vertical_margin"
              android:paddingLeft="@dimen/activity_horizontal_margin"
              android:paddingRight="@dimen/activity_horizontal_margin"
              android:paddingTop="@dimen/activity_vertical_margin"
              tools:context="com.example.retrofitdemo.MainActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World!"/>

    <Button
        android:id="@+id/login"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="UserLoginTask"/>

    <Button
        android:id="@+id/login2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Update"/>

    <Button
        android:id="@+id/login3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Update execute"/>

    <Button
        android:id="@+id/login4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MeizhiClassify"/>

    <Button
        android:id="@+id/login5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MeizhiClassify Cancel"/>

    <Button
        android:id="@+id/login6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Login6"/>

    <Button
        android:id="@+id/login7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="号码归属地"/>

    <Button
        android:id="@+id/login8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SSL TLS"/>
</LinearLayout>
